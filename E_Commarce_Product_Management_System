class Product {
	int productId;
	String productName;
	int price;
	int quantity;
	Product(int productId,String productName,int price, int quantity) {
		this.productId=productId;
		this.productName=productName;
		this.price=price;
		this.quantity=quantity;
	}
	public void addProduct(int productId,String productName,int price,int quantity) {
		if(price<0){
		    throw new InvalidProductException("price is not negative");
		}
			System.out.println("Procuct id is: "+productId+" product name is: "
			                   +productName+" and price is: "+price
			                   +" product quantity is + "+quantity);
	}
	
class InvalidProductException extends Exception{
    public InvalidProductException(String message){
        super(message);
    }
}


public void addProduct(String productName,int price) {
	System.out.println("product name is: "+productName+" and price is: "+price);
}
public void addProduct(String name,int price,int quantity) {
	System.out.println(" product name is "+name+" price: "+price+" quantity: "+quantity);
}
}



public class Main {
	public static void main(String arge[]) {
	    try{
		Product pd =new Product(1,"Mobile",-19999,5);
		pd.addProduct(1,"Freeze",-30000,2);
		pd.addProduct("Mobile",-15999);
		pd.addProduct("TV",-3000,3);
	    }catch(InvalidProductException e){
	        System.out.println("Exception: "+e.getMessage());
	    }

	}
}
